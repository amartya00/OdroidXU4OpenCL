    void KernelExecutor<x, y, z, T0, T1, T2, T3, T4, T5, T6, T7, T8, T9>::operator () (T0 arg0, T1 arg1, T2 arg2, T3 arg3, T4 arg4, T5 arg5, T6 arg6)
    {
